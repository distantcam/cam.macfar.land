{
	"private": "true",
	"scripts": {
		"start": "run-p start:*",
		"start:eleventy": "eleventy --serve",
		"start:postcss": "postcss styles/site.css --o _tmp/site.css --watch",
		"build": "run-s build:postcss build:eleventy",
		"build:eleventy": "cross-env ELEVENTY_PRODUCTION=true eleventy",
		"build:postcss": "cross-env NODE_ENV=production postcss styles/site.css --o _site/site.css",
		"clean": "rimraf _site _tmp"
	},
	"devDependencies": {
		"@11ty/eleventy": "^0.11.0",
		"cheerio": "^1.0.0-rc.3",
		"cross-env": "^7.0.2",
		"dotenv": "^8.2.0",
		"isomorphic-fetch": "^2.2.1",
		"luxon": "^1.24.1",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^7.1.1",
		"postcss-nested": "^4.2.3",
		"rimraf": "^3.0.2",
		"simple-icons": "^3.2.0",
		"tailwindcss": "^1.5.2",
		"unsplash-js": "^6.0.0"
	}
}
